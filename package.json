{
  "name": "node-file-cli",
  "main": "index.js",
  "scripts": {
    "dev": "node index.js",
    "lint": "eslint *.js",
    "precommit": "lint-staged && npm run lint"
  },
  "bin": {
    "node-file": "index.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "commander": "^7.1.0",
    "download-git-repo": "^3.0.2",
    "form-data": "^4.0.0",
    "inquirer": "^8.0.0",
    "jszip": "^3.6.0",
    "ora": "^6.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.17.0",
    "eslint-config-standard": "^16.0.2",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "standard": "^16.0.3"
  }
}
