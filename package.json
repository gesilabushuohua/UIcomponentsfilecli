{
  "name": "node-file-cli",
  "main": "index.js",
  "scripts": {
    "dev": "node index.js",
    "lint": "eslint index.js src",
    "precommit": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "eslint --fix",
      "git add"
    ]
  },
  "bin": {
    "node-file": "index.js"
  },
  "dependencies": {
    "commander": "^7.1.0"
  },
  "devDependencies": {
    "eslint": "^7.17.0",
    "eslint-config-standard": "^16.0.2",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "standard": "^16.0.3"
  }
}
